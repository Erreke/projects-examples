{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  },
  "extends": ["eslint:recommended", "airbnb"],
  "settings": {
    "import/resolver": {
      "node": {},
      "webpack": {}
    }
  }
}